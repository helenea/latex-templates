\section{Contexte et objectifs}
\subsection{Interactions}

\begin{frame}{Contexte}
	\begin{itemize}
		\item Politiques de santé publique $\rightarrow$ action sur un pathogène à la fois\\
		(vaccins, antibiotiques...)\\
		\item Co-circulation de plusieurs pathogènes dans une population\\
		\medskip
		{\color{myblue}$\rightarrow$ \textbf{interactions possibles lors de l'infection de l'homme}}\\
	\end{itemize}
	
	\medskip
	\textbf{Interactions grippe $\rightarrow$ pneumocoque souvent suggérées}\\
	\begin{itemize}
		\item mises en évidence : modèles animaux, niveau individuel homme
		\item controversées au niveau populationnel
	\end{itemize}
	\textit{{\scriptsize Opatowski \textit{et al.}, 2013 ; Walter \textit{et al.}, 2010 ; Nicoli \textit{et al.}, 2013}}
\end{frame}


\subsection{Pathogènes}

\begin{frame}{Le pneumocoque et les infections virales}
	\begin{block}{Bactérie \textit{Streptococcus pneumoniae} (pneumocoque)}
	\begin{itemize}
		\item Colonisation du nasopharynx et de la gorge
		\item Transmission aérienne
		\item Peut mener à des infections
	\end{itemize}
	
	\textbf{Problème majeur de santé publique}\\
	$\rightarrow$ Infections sévères (pneumonie, méningite...) : 5 millions de décès chez les enfants de moins de 5 ans chaque année {\scriptsize \textit{(OMS)}}
	\end{block}
	
	\begin{block}{Virus de la grippe}
	\begin{itemize}
		\item Infection des voies respiratoires
		\item Transmission aérienne
	\end{itemize}
	\end{block}
\end{frame}

\begin{frame}{Données de surveillance du Centre National de Référence des Pneumocoques (CNRP)}
	\begin{figure}
		\includegraphics[scale = 0.38]{Images/for_Helene.pdf}
	\end{figure}
\end{frame}


 \begin{frame}{Données de surveillance du réseau Sentinelles}
 	\vspace{-1cm}
 	\begin{figure}
 		\includegraphics{Images/sentinelles.png}
 	\end{figure}
 \end{frame}


\subsection{Objectifs}
\begin{frame}{Problématique}
	Nécessité d'une approche globale dans les politiques de santé publique\\
	$\rightarrow$ tenir compte des interactions entre pathogènes\\
	\bigskip
	{\color{myblue}\textbf{Problématique :}}\\
	Pas de consensus sur la meilleure approche méthodologique pour détecter, quantifier, identifier et modéliser les interactions entre les virus respiratoires et le pneumocoque au niveau populationnel.
\end{frame}


\begin{frame}{Objectifs}
	\begin{itemize}
		\item[A.] Proposer un simulateur basé sur un modèle dynamique de co-circulation de pathogènes en interaction
		\bigskip
		\item[B.] Comparer et évaluer les méthodes d'analyse des interactions à partir de données de surveillance
		\bigskip
		\item[C.] Analyser le comportement des modèles dynamiques à deux souches
	\end{itemize}
\end{frame}


\section[Simulateur]{Simulateur de pathogènes co-circulant et en interaction}

\begin{frame}{Modèle agent de co-circulation de pathogènes en interaction}
	\begin{changemargin}{2cm}{0cm}
	\begin{itemize}
	\item[Principe] Modèle défini au niveau individuel selon des règles de comportement
	\item[] Observations faites au niveau populationnel
	\end{itemize}
	\end{changemargin}
	
	\bigskip
	\begin{center}
	\color{myblue}{\textbf{$\rightarrow$ Émergence d'une dynamique globale}}
	\end{center}
	
	\begin{alertblock}{But}
		Simuler de manière stochastique la co-circulation de la grippe et d'un autre pathogène (épidémique ou endémique) en interaction dans une population humaine virtuelle.
	\end{alertblock}
\end{frame}


