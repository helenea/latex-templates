\subsection{Régression}
\begin{frame}{Analyse des régressions sur les scénarios de simulations}
	\textbf{Modèle de régression}
	\begin{eqnarray*}
		G(PI(t)) &=& a + bF(t+i) + c\cos{\dfrac{2\pi t}{52}} + d\sin{\dfrac{2\pi t}{52}} + \epsilon
	\end{eqnarray*}
	
	\only<1-3>{
	\begin{columns}
	\begin{column}{5.5cm}
	\begin{table}[h]
	\begin{flushleft}
	\begin{tabular}{cl}
  		\makecell*{$PI$} & \makecell*[l]{pneumococcal infections}\\
  		\makecell*{$F$} & \makecell*[l]{cas de grippe (flu)}\\
  		\makecell*{$t$} & \makecell*[l]{semaine}\\
  		\makecell*{$i$} & \makecell*[l]{décalage hebdomadaire}\\
  	\end{tabular}
	\end{flushleft}
  	\end{table}
  	\end{column}
  	
  	\begin{column}{5.5cm}
  	\only<2>{
  		\begin{center}
  		$i\in[-10;10]$
  		\end{center}
  		$i<0\rightarrow$ test l'effet de $F$ sur $PI$\\
  		$i>0\rightarrow$ vérifier le sens de l'interaction
  	}
  	\only<3>{
  		\begin{center}
  		\textbf{Critère de comparaison}
  		\end{center}
  		t-test de Student : $t = \dfrac{\hat{b}}{std\ dev}$
  	}
  	\end{column}
  	\end{columns}
  	}
	
    \only<4>{
