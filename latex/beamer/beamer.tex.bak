\subsection{Fit de modèles dynamiques}
\begin{frame}{Ajustement de modèles dynamiques}
	\textbf{Modèle de transmission du pneumocoque}
	{\scriptsize \textit{Opatowski \textit{et al.}, 2013}}
%	\begin{figure}
%		\includegraphics[scale=0.3]{Images/Analyses/modele_lulla.png}
%	\end{figure}

	\begin{columns}
	\begin{column}{6.5cm}
	\begin{eqnarray*}
		\frac{dS}{dt} &=& \lambda * C(t) - \beta (t)*C(t)*S(t)\\
		\frac{dC}{dt} &=& -\lambda * C(t) + \beta (t)*C(t)*S(t)\\
		dI_{cumul} &=& p(t)*C(t)
	\end{eqnarray*}
	
	Avec 2 paramètres d'interaction $\xi$ et $\pi$ :
	\begin{eqnarray*}
		\beta (t) &=& \beta _{0}(1+\xi *PI(t))\\
		p(t) &=& p_{0}(1+\pi *PI(t))
	\end{eqnarray*}
	\end{column}
	
	\begin{column}{4.2cm}
		\begin{table}[h]
		\begin{center}
		\begin{tabular}{|c|l|}
			\hline
			\makecell*{$\beta _{0}$} & \makecell*{estimé}\\
			\hline
			\makecell*{$p_{0}$} & \makecell*{estimé}\\
			\hline
			\makecell*{$\xi$} & \makecell*{\textbf{estimé}}\\
			\hline
			\makecell*{$\pi$} & \makecell*{\textbf{estimé}}\\
			\hline
			\makecell*{$\dfrac{1}{\lambda}$} & \makecell*{3 semaines}\\
			\hline
		\end{tabular}
		\end{center}
		\end{table}
	\end{column}
	\end{columns}
\end{frame}


\begin{frame}{Résultats préliminaires pour le paramètre d'interaction $\xi$}
	\textbf{Méthode d'ajustement:} algorithme MCMC avec une vraisemblance de Poisson, scénario $\alpha=10$\\
	
    \bigskip
	\begin{table}[h]
	\begin{center}
	\begin{tabular}{|c|c|c|}
		\hline
        \makecell*{Paramètre estimé} & \makecell*{Valeur moyenne} & \makecell*{CI 95\%}\\
        \hline
		\makecell*{$\beta _{0}$} & \makecell*{\num{4.56e-6}} & \makecell*{[\num{4.49e-6} ; \num{4.65e-6}]}\\
		\hline
		\makecell*{$p_{0}$} & \makecell*{\num{2.51e-4}} & \makecell*{[\num{2.41e-4} ; \num{2.61e-4}]}\\
		\hline
		\makecell*{$\xi$} & \makecell*{\num{27.94}} & \makecell*{[\num{15.61} ; \num{38.92}]}\\
		\hline
	\end{tabular}
	\end{center}
	\end{table}
    
    \begin{figure}
		\includegraphics[scale=0.5]{Images/Analyses/fit_xi.png}
	\end{figure}
\end{frame}


